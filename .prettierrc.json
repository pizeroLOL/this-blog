{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "quoteProps": "as-needed",
  "endOfLine": "lf",
  "tabWidth": 2,
  "trailingComma": "es5",
  "plugins": ["prettier-plugin-toml", "prettier-plugin-astro"],
  "overrides": [
    {
      "files": ["*.astro"],
      "options": {
        "parser": "astro"
      }
    }
  ]
}
